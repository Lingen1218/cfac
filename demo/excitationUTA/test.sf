# Calculate the electron impact excitation cross sections
# in the UTA mode.

SetUTA(1)

SetAtom('Fe')

# 1s shell is closed
Closed('1s')

Config('2*8', group = 'n2')
Config('2*7 3*1', group = 'n3')

# Self-consistent iteration for optimized central potential
SetScreening([10])
ConfigEnergy(0)
OptimizeRadial('n2')
ConfigEnergy(1)

Structure('lev.bin')
MemENTable('lev.bin')
PrintTable('lev.bin', 'lev.asc', 1)

CETable('ce.bin', ['n2'], ['n3']) 
PrintTable('ce.bin', 'ce.asc', 1) 
